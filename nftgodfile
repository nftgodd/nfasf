document.querySelectorAll('.faq__question__title').forEach(function(el) {
  el.addEventListener('click', event => {
    document.querySelectorAll('.faq__question__answer').forEach(function(item) {
      item.style.height = 0;
    });
    var height = el.parentNode.querySelector('.faq__question__answer p').offsetHeight + 50;
    height += "px";
    el.parentNode.querySelector('.faq__question__answer').style.height = height;
  });
});
</script>


